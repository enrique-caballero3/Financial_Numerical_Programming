   "source": [
    "# Calculate the value of a stock for a constant growth firm\n",
    "#\n",
    "# Inputs\n",
    "re = 0.08\n",
    "g = 0.02\n",
    "Div0 = 5\n",
    "#\n",
    "# Calculate Stock Price\n",
    "Stock_Price = Div0 * (1 + g) / (re - g)\n",
    "print(\"The theoretical value of the Stock is\", Stock_Price)"
   ]
